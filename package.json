{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && react-snap",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/about",
      "/compare",
      "/quotes",
      "/how-it-works",
      "/contact",
      "/faq",
      "/what-is-health-insurance",
      "/health-insurance-faq",
      "/reviews",
      "/guides",
      "/blog",
      "/reviews/southern-cross",
      "/reviews/nib",
      "/reviews/aia",
      "/reviews/partners-life",
      "/reviews/accuro",
      "/reviews/unimed",
      "/best-health-insurance-companies-nz",
      "/cheapest-health-insurance-nz",
      "/private-health-insurance",
      "/health-insurance-teachers-nz",
      "/health-insurance-nurses-nz",
      "/health-insurance-tradies-nz",
      "/health-insurance-contractors-nz",
      "/health-insurance-farmers-nz",
      "/health-insurance-self-employed-nz",
      "/health-insurance-cost-calculator-nz",
      "/health-insurance-tax-deductions-nz",
      "/cheapest-family-health-insurance-nz",
      "/health-insurance-cancer-cover-nz",
      "/health-insurance-diabetes-nz",
      "/health-insurance-mental-health-nz",
      "/health-insurance-pregnancy-nz",
      "/health-insurance-heart-conditions-nz",
      "/health-insurance-pre-existing-conditions-nz",
      "/health-insurance-vs-acc-nz",
      "/private-vs-public-healthcare-nz",
      "/health-insurance-waiting-periods-explained",
      "/health-insurance-excess-guide-nz",
      "/health-insurance-claims-process-nz",
      "/health-insurance-terminology-guide"
    ]
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.29.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
